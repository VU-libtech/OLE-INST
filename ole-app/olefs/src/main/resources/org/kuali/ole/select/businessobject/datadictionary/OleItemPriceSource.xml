  <bean id="OleItemPriceSource" parent="OleItemPriceSource-parentBean"/>

  <bean id="OleItemPriceSource-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.ole.select.businessobject.OleItemPriceSource"/>
    <property name="inquiryDefinition">
      <ref bean="OleItemPriceSource-inquiryDefinition"/>
    </property>
    <property name="lookupDefinition">
      <ref bean="OleItemPriceSource-lookupDefinition"/>
    </property>
    <property name="titleAttribute" value="itemPriceSourceId"/>
    <property name="objectLabel" value="ItemPriceSource"/>
    <property name="attributes">
      <list>
        <ref bean="OleItemPriceSource-itemPriceSourceId"/>
        <ref bean="OleItemPriceSource-itemPriceSource"/>
        <ref bean="OleItemPriceSource-active"/>
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="OleItemPriceSource-itemPriceSourceId" parent="OleItemPriceSource-itemPriceSourceId-parentBean"/>

  <bean id="OleItemPriceSource-itemPriceSourceId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemPriceSourceId"/>
    <property name="forceUppercase" value="true"/>
    <property name="label" value="Item Price Source Code"/>
    <property name="shortLabel" value="Code"/>
    <property name="maxLength" value="8"/>
    <property name="validationPattern">
      <ref bean="NumericValidation" />
    </property>
     <property name="control">
      <ref bean="HiddenControl" />
    </property>
  </bean>
  <bean id="OleItemPriceSource-itemPriceSource" parent="OleItemPriceSource-itemPriceSource-parentBean"/>

  <bean id="OleItemPriceSource-itemPriceSource-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="itemPriceSource"/>
    <property name="label" value="Item Price Source"/>
    <property name="shortLabel" value="Description"/>
    <property name="maxLength" value="45"/>
    <property name="validationPattern">
      <ref bean="AnyCharacterWithWhitespaceValidation" />
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="47"/>
    </property>
  </bean>
  
  <bean id="OleItemPriceSource-active" parent="OleItemPriceSource-active-parentBean"/>
  <bean id="OleItemPriceSource-active-parentBean" abstract="true" parent="GenericAttributes-activeIndicator">
    <property name="name" value="active"/>
  </bean>
  

<!-- Business Object Inquiry Definition -->


  <bean id="OleItemPriceSource-inquiryDefinition" parent="OleItemPriceSource-inquiryDefinition-parentBean"/>

  <bean id="OleItemPriceSource-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Item Price Source Inquiry"/>
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value=""/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="itemPriceSourceId"/>
              <bean parent="FieldDefinition" p:attributeName="itemPriceSource"/>
              <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->


  <bean id="OleItemPriceSource-lookupDefinition" parent="OleItemPriceSource-lookupDefinition-parentBean"/>

  <bean id="OleItemPriceSource-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Item Price Source Lookup"/>
    
    <property name="defaultSort">
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="itemPriceSourceId"/>
        <bean parent="FieldDefinition" p:attributeName="itemPriceSource"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="itemPriceSourceId"/>
        <bean parent="FieldDefinition" p:attributeName="itemPriceSource"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
  </bean>
</beans>
