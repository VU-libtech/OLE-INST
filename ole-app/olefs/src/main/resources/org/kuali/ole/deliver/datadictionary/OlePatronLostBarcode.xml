    </property>-->
    <property name="titleAttribute" value="olePatronLostBarcodeId"/>
    <property name="objectLabel" value="Lost Barcode"/>

    <property name="attributes">
      <list>
        <ref bean="OlePatronLostBarcode-olePatronLostBarcodeId"/>
