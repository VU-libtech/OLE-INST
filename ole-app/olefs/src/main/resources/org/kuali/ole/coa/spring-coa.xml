            <ref bean="indirectCostRecoveryRateDetailFiscalYearMaker" />
            <ref bean="indirectCostRecoveryRateFiscalYearMaker" />
          </list>	
        </property>
        <property name="batchFileDirectories">
          	<list>
            	<value>${reports.directory}/coa</value>
         	</list>
        </property>
        <property name="resourceBundleNames">
            <list>
                <value>org/kuali/ole/locale/${ole.locale.properties.folder}/Labels-Coa</value>
            </list>
        </property>
	</bean>

