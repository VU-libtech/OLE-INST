    <bean id="OLEBatchProcessProfileBo-InquiryView-mainSection" parent="Uif-Disclosure-VerticalBoxSection">
        <property name="headerText" value="Main Section" />
        <property name="items">
            <list>
                <bean parent="Uif-GridGroup" p:layoutManager.numberOfColumns="2">
                    <property name="items">
                        <list>
                            <bean id="oleBatchProcessProfileBoInquiry_batchProcessProfileId" parent="Uif-DataField" p:label="Batch Process Profile Id" p:propertyName="batchProcessProfileId"/>
