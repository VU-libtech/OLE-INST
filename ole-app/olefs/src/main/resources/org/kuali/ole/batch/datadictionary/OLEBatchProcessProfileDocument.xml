                            <bean id="oleBatchProcessProfileBoInquiry_dataToImport" parent="Uif-DataField" p:propertyName="dataToImport" p:label="Data To Import" p:render="@{dataObject.batchProcessProfileType eq 'Bib Import'}"/>
                            <bean id="oleBatchProcessProfileBoInquiry_requisitionsForTitle" parent="Uif-DataField" p:propertyName="requisitionsforTitle" p:label="Requisitions For Titles" p:render="@{dataObject.batchProcessProfileType eq 'Order Record Import'}"/>
                            <bean id="oleBatchProcessProfileBoInquiry_dataToExport" parent="Uif-DataField" p:label="Data To Export" p:propertyName="dataToExport" p:render="@{dataObject.batchProcessProfileType eq 'Batch Export'}"/>
                            <bean id="oleBatchProcessProfileBoInquiry_exportScope" parent="Uif-DataField" p:label="Export Scope" p:propertyName="exportScope" p:render="@{dataObject.batchProcessProfileType eq 'Batch Export'}"/>
                        </list>
