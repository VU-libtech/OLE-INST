                    <property name="optionsFinder">
                        <bean class="org.kuali.ole.batch.keyvalue.OLEBatchProcessTypeValuesFinder" />
                    </property>
                    <property name="control">
                        <bean parent="Uif-DropdownControl" p:cssClasses="fixed-size-200-select"/>
                    </property>
                    <property name="quickfinder">
                        <null></null>
                    </property>
                </bean>
                <bean id="lookup-krmsProfileName" parent="Uif-LookupCriteriaInputField" p:propertyName="krmsProfileName" p:label="Batch Process KRMS Profile">
                    <property name="optionsFinder">
                        <bean class="org.kuali.ole.ingest.keyvalue.AgendaKeyValues" />
                    </property>
                    <property name="control">
                        <bean parent="Uif-DropdownControl" p:cssClasses="fixed-size-200-select"/>
                    </property>
                    <property name="quickfinder">
                        <null></null>
                    </property>
                </bean>
            </list>
        </property>

        <property name="resultFields">
            <list>
                <bean id="result-batchProcessProfileId" parent="Uif-DataField" p:propertyName="batchProcessProfileId" />
                <bean id="result-batchProcessProfileName" parent="Uif-DataField" p:propertyName="batchProcessProfileName" />
