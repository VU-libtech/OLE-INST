                          value="@{#ConfigProperties['externalizable.help.url']}/reference/webhelp/BASICS/content/ch02s04.html#_lookupHelp"/>
                <property name="tooltipHelpContent" value="[Help] help"/>
            </bean>
        </property>
        <property name="dataObjectClassName" value="org.kuali.ole.batch.bo.OLEBatchProcessProfileBo" />
        <property name="viewHelperServiceClass" value="org.kuali.ole.batch.lookup.OLEBatchProcessProfileLookupableImpl"/>
        <property name="criteriaFields">
            <list>
                <bean id="lookup-batchProcessProfileName" parent="Uif-LookupCriteriaInputField" p:propertyName="batchProcessProfileName" p:label="Batch Process Profile Name"/>
