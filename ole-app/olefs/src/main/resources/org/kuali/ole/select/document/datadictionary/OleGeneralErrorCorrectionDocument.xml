<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:dd="http://rice.kuali.org/dd"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://rice.kuali.org/dd http://rice.kuali.org/dd/dd.xsd">
	
	<bean id="GeneralErrorCorrectionDocument" parent="OleGeneralErrorCorrectionDocument-parentBean" />

	<alias name="GeneralErrorCorrectionDocument" alias="OleGeneralErrorCorrectionDocument"/>

	<bean id="OleGeneralErrorCorrectionDocument-parentBean" abstract="true" parent="GeneralErrorCorrectionDocument-parentBean">
		<property name="documentClass"
			value="org.kuali.ole.select.document.OleGeneralErrorCorrectionDocument" />
		<property name="baseDocumentClass"
			value="org.kuali.ole.fp.document.GeneralErrorCorrectionDocument" />
	</bean> 
  
	
</beans>