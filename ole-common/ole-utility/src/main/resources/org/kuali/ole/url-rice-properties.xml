<!--
  URL related properties that are re-used in more than one OLE application must reside here
-->

<config>
    <!-- Default URL's for local development -->
    <param name="ole.url.base">http://localhost:8080</param>
    <param name="ole.docstore.url.base">${ole.url.base}/oledocstore</param>
    <param name="ole.fs.url.base">${ole.url.base}/olefs</param>

    <!-- General environments as known to external team members aka "dev, tst, demo" -->
    <param name="dev.ole.docstore.url.base">http://dev.docstore.ole.kuali.org</param>
    <param name="dev.ole.fs.url.base">http://dev.ole.kuali.org</param>

    <param name="staging.ole.docstore.url.base">http://docstore.staging.ole.kuali.org</param>
    <param name="staging.ole.fs.url.base">http://staging.ole.kuali.org</param>

    <param name="qa.ole.docstore.url.base">http://docstore.qa.ole.kuali.org</param>
    <param name="qa.ole.fs.url.base">http://qa.ole.kuali.org</param>

    <param name="tst.ole.docstore.url.base">http://tst.docstore.ole.kuali.org</param>
    <param name="tst.ole.fs.url.base">http://tst.ole.kuali.org</param>

    <param name="demo.ole.docstore.url.base">http://docstore.demo.ole.kuali.org</param>
    <param name="demo.ole.fs.url.base">http://demo.ole.kuali.org</param>

    <!-- Properties targeted at specific servers. Coordinate with devops staff before editing these -->
    <param name="env1.ole.fs.url.base">${dev.ole.fs.url.base}</param>
    <param name="env1.ole.docstore.url.base">${dev.ole.docstore.url.base}:8080</param>

    <param name="env2.ole.fs.url.base">${dev.ole.fs.url.base}</param>
    <param name="env2.ole.docstore.url.base">${dev.ole.docstore.url.base}:8080</param>

    <param name="env4.ole.fs.url.base">${tst.ole.fs.url.base}</param>
    <param name="env4.ole.docstore.url.base">${tst.ole.docstore.url.base}</param>

    <param name="env5.ole.fs.url.base">${tst.ole.fs.url.base}</param>
    <param name="env5.ole.docstore.url.base">${tst.ole.docstore.url.base}</param>

    <param name="env7.ole.fs.url.base">${demo.ole.fs.url.base}</param>
    <param name="env7.ole.docstore.url.base">${demo.ole.docstore.url.base}:8080</param>

    <param name="env8.ole.fs.url.base">${demo.ole.fs.url.base}</param>
    <param name="env8.ole.docstore.url.base">${demo.ole.docstore.url.base}:8080</param>

    <param name="env9.ole.fs.url.base">${qa.ole.fs.url.base}</param>
    <param name="env9.ole.docstore.url.base">${qa.ole.docstore.url.base}</param>

    <param name="env10.ole.fs.url.base">${qa.ole.fs.url.base}</param>
    <param name="env10.ole.docstore.url.base">${qa.ole.docstore.url.base}</param>

    <param name="env11.ole.fs.url.base">${staging.ole.fs.url.base}</param>
    <param name="env11.ole.docstore.url.base">${staging.ole.docstore.url.base}</param>

    <param name="env12.ole.fs.url.base">${staging.ole.fs.url.base}</param>
    <param name="env12.ole.docstore.url.base">${staging.ole.docstore.url.base}</param>

</config>